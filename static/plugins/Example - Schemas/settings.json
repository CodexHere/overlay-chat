            },
            {
              "name": "simpleInputs-password",
              "inputType": "password"
            },
            {
              "name": "simpleInputs-number",
              "inputType": "number",
              "min": 123,
              "max": 456,
              "step": 3
            },
            {
              "name": "simpleInputs-checkbox",
              "inputType": "checkbox"
            },
            {
              "name": "simpleInputs-switch",
              "inputType": "switch"
            },
            {
