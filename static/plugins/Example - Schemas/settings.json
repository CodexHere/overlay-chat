{
  "label": "Gamut Settings Test",
  "name": "gamutSettings",
  "inputType": "group-subschema",
  "subSchema": [
    {
      "name": "testArea1",
      "inputType": "group-subschema",
      "label": "Simple Group-SubSchema Demonstration",
      "description": "This Grouping represents all the supported HTML Form/Input Elements this library provides. The Group-SubSchema (<code>group-subschema</code>) built here shows how you can nest and group Settings within a collapsable area.",
      "tooltip": "A nesting and grouping of Settings",
      "subSchema": [
        {
          "name": "simpleInputs",
          "inputType": "group-subschema",
          "label": "Simple Inputs",
          "description": "These are single-value inputs, and the simplest of User Inputs.",
          "tooltip": "The most common Inputs for getting data from Users",
          "subSchema": [
            {
              "name": "simpleInputs-text-disabled",
              "inputType": "text",
              "isDisabled": true,
              "defaultValue": "This is Disabled"
            },
            {
              "name": "simpleInputs-text-readonly",
              "inputType": "text",
              "isReadOnly": true,
              "defaultValue": "This is ReadOnly"
            },
            {
              "name": "simpleInputs-text",
